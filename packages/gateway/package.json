{
  "name": "@oasislabs/gateway",
  "version": "1.0.0-rc.17",
  "description": "",
  "keywords": [],
  "main": "dist/index.umd.js",
  "module": "dist/index.es5.js",
  "typings": "dist/lib/src/index.d.ts",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "author": "Oasis Labs Inc. <info@oasislabs.com>",
  "scripts": {
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "lint:fix": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts' --fix",
    "prebuild": "rimraf dist",
    "build": "yarn build:node && yarn build:browser",
    "build:node": "tsc -b && rollup -c rollup/rollup.config.ts",
    "build:browser": "tsc -b && rollup -c rollup/rollup.config.browser.ts",
    "test": "jest test",
    "coverage": "jest test --coverage"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "dependencies": {
    "@oasislabs/common": "^1.0.0-rc.9",
    "@oasislabs/service": "^1.0.0-rc.14",
    "@oasislabs/types": "^1.0.0-rc.2",
    "@types/uuid": "^3.4.5",
    "axios": "^0.18.0"
  },
  "browser": {
    "./dist/index.umd.js": "./dist/index.browser.umd.js",
    "./dist/index.es5.js": "./dist/index.browser.es5.js"
  }
}
